@prefix ex: <http://example.org/pupuh#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Definisi Ontologi (Classes)
ex:Paragraf a owl:Class ;
    rdfs:label "Paragraf"@id ;
    rdfs:comment "Representasi dari satu paragraf dalam Pupuh."@id .

ex:Kata a owl:Class ;
    rdfs:label "Kata"@id ;
    rdfs:comment "Representasi dari satu kata dalam Pupuh."@id .

# Definisi Ontologi (Properties)
ex:isiLatin a rdf:Property ;
    rdfs:label "isi Latin"@id ;
    rdfs:comment "Isi paragraf atau kata dalam aksara Latin Jawa."@id ;
    rdfs:domain ex:Paragraf, ex:Kata ;
    rdfs:range xsd:string .

ex:isiAksaraJawa a rdf:Property ;
    rdfs:label "isi Aksara Jawa"@id ;
    rdfs:comment "Isi paragraf atau kata dalam aksara Jawa."@id ;
    rdfs:domain ex:Paragraf, ex:Kata ;
    rdfs:range xsd:string .

ex:arti a rdf:Property ;
    rdfs:label "Arti"@id ;
    rdfs:comment "Terjemahan atau makna dari paragraf atau kata."@id ;
    rdfs:domain ex:Paragraf, ex:Kata ;
    rdfs:range xsd:string .

ex:munculDalamParagraf a rdf:Property ;
    rdfs:label "muncul dalam paragraf"@id ;
    rdfs:comment "Menunjukkan paragraf di mana sebuah kata muncul."@id ;
    rdfs:domain ex:Kata ;
    rdfs:range ex:Paragraf .

